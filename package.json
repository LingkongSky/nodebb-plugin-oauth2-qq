{
  "name": "nodebb-plugin-oauth2-qq",
  "version": "0.6.0",
  "description": "This plugin work on Nodebb V3.x,support user login & register via Tencent.",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LingkongSky/nodebb-plugin-oauth2-qq.git"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "oauth2-qq",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LingkongSky/nodebb-plugin-oauth2-qq/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.7.0 || ^3.6.0 || ^3.5.0 || ^3.4.0 || ^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-angular": "17.7.0",
    "eslint": "8.x",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.x",
    "husky": "8.0.3",
    "lint-staged": "14.0.1"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
